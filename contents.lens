<koken:include file="inc/components/header.html" />
<main role="main">
    <koken:load limit="18" infinite="true">
      <ol class="grid">
        <koken:loop>
          <li>
            <koken:link lightbox="true">
              <koken:img crop="adaptive"/>
              <div>
                <h3>{{ caption }} </h3>
              </div>
            </koken:link>
          </li>
        </koken:loop>
      </ol>
    <koken:else>
      <koken:note>
        No images or videos found
      </koken:note>
    </koken:load>
    </main>
    <footer>
      {{ site.copyright }}
    </footer>
  </body>
</html>
